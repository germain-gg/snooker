{
  "compilerOptions": {
    "target": "ESNext",                                
    "lib": ["ESNext"],                                 
    "module": "commonjs",                              
    "esModuleInterop": true,                           
    "forceConsistentCasingInFileNames": true,       
    "strict": true,                                 
    "noImplicitAny": true,                          
    "strictNullChecks": true,                       
    "skipLibCheck": true                            
  }
}
